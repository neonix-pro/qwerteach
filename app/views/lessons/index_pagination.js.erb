<% unless @history_lessons.nil? %>
  $('#history').html("<%= j render partial: 'lessons/index/history' %>");
<% end %>
<% unless @pending_lessons.nil? %>
  $('#pending').html("<%= j render partial: 'lessons/index/pending' %>");
<% end %>
<% unless @planned_lessons.nil? %>
  $('#planned').html("<%= j render partial: 'lessons/index/planned' %>");
<% end %>
<% unless @lesson_packs.nil? %>
  $('#packs').html("<%= j render partial: 'lessons/index/packs', locals: {packs: @lesson_packs} %>");
<% end %>
