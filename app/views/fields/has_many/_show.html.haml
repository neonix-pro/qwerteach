- if field.resources.any?
  = render(                                              |
      'admin/application/collection',                                      |
      collection_presenter: field.associated_collection, |
      resources: field.resources(params[field.name]),    |
      table_title: field.name                            |
    )                                                    |
  - if field.more_than_limit?
    = paginate_admin field.resources(params[field.name]), param_name: "#{field.name}"
- else
  = t('administrate.fields.has_many.none', default: "â€“")
