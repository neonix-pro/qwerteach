%meta{:content => user_signed_in? ? current_user.id : "", :name => "user-id"}/
%meta{:content => user_signed_in? ? current_user.email : "", :name => "user-email"}/
%meta{:content => user_signed_in? ? current_user.name : "", :name => "user-name"}/
%meta{:content => "width=device-width, initial-scale=1", :name => "viewport"}/
%title Qwerteach
= javascript_include_tag 'application', 'data-turbolinks-track' => true
= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true
= stylesheet_link_tag "https://fonts.googleapis.com/icon?family=Material+Icons", rel:"stylesheet", 'data-turbolinks-track' => false
= favicon_link_tag 'logo.ico'
= csrf_meta_tags
= render partial: 'layouts/google_analytics'
= yield(:head)