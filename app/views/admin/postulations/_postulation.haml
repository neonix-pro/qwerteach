%h4 Responsable: #{postulation.responsible_admin.name unless postulation.responsible_admin.nil?}
%table.table.table-striped.table-bordered.table-hover
  %thead
    %tr
      - postulation.admin_fields.keys.each do |key|
        %th.cell-label #{key.to_s.titleize}
    %tr
      -postulation.admin_fields.each do |key, attr|
        %td #{attr ? 'OK' : 'X'}
